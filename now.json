{
    "name": "ipapi",
    "version": 2,
    "public": false,
    "alias": "ipinfo.now.sh",
    "regions": ["bru", "sfo", "hnd"],
    "github": {
    	"enabled": true,
    	"silent": true
    },
    "builds": [
      {
        "src": "index.js",
        "use": "@now/node"
      }
    ],
    "routes": [
    	{ "src": "/(?<ip>[^/]+)", "dest": "/" }
    ],
    "env": {
    	"DATABASE_URL": "@ipapi-db-url"
    }
}
